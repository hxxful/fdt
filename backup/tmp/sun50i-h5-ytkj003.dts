				cpu_crit {
					temperature = <0x19a28>;
					hysteresis = <0x7d0>;
					type = "critical";
				};
			};

			cooling-maps {

				cpu_warm_limit_cpu {
					trip = <0x28>;
					cooling-device = <0x29 0xffffffff 0x1>;
				};

				cpu_hot_limit_cpu {
